bool checkIfExist(int* arr, int arrSize) {
    
    for(int i = 0; i < arrSize; i++){
        for(int j = 0; j < arrSize; j++){
            if( 2 * arr[j] == arr[i] && j != i){
                printf(" 2 * %d = %d ",arr[j], arr[i]);
                return true;
            }
        }
    }
    return false;
}

